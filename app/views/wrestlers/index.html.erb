<% content_for :title do %>
	Wrestlers
<% end %>

<div class = "container">
    <div class = "jumbotron text-center">
        <h1>Team Lineup</h1>
        <p>2013-2014</p>
    </div>
</div>

<div class = "container">
    <div class = "row text-center" id = "grid">
		<% @wrestlers.each do |wrestler| %>
	    	<div class = "col-md-3 col-sm-4 col-xs-6 listing-item">
	    		<%= image_tag "lbarnes.jpg", class: "img-responsive center-block" %>
	    		<h4><%= wrestler.name %></h4>
	    		<h5><%= wrestler.year %></h5>
	    		<h6>CS Masters Finish - <%= wrestler.masters %></h6>
	    		<h6>State Finish - <%= wrestler.state %></h6>
	    	</div>
    	<% end %>
    </div>
</div>